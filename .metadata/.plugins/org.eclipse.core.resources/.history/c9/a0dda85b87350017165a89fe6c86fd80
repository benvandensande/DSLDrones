grammar org.xtext.example.mydsl.MyTerminals

import "http://www.eclipse.org/emf/2002/Ecore" as ecore
 

terminal ID: 
    '^'?('a'..'z'|'A'..'Z'|'_')('a'..'z'|'A'..'Z'|'_'|'0'..'9')*;
    

terminal STRING: 
    '"' ( '\\'('b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\') | !('\\'|'"') )* '"' |
    "'" ( '\\'('b'|'t'|'n'|'f'|'r'|'u'|'"'|"'"|'\\') | !('\\'|"'") )* "'";
 

terminal DOUBLE returns ecore::EBigDecimal:  ('-')?('0'..'9')* ('.' ('0'..'9')+)?;
        
